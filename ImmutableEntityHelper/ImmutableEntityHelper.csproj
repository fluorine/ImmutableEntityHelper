    <AssemblyName>ImmutableEntityHelper</AssemblyName>
    <RootNamespace>ImmutableEntityHelper</RootNamespace>
    <RepositoryUrl>https://github.com/fluorine/ImmutableEntityHelper</RepositoryUrl>
    <PackageReleaseNotes>Initial version.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/fluorine/ImmutableEntityHelper</PackageProjectUrl>
  </PropertyGroup>

